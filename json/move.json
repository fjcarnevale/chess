{% autoescape true %}
{
   {% if success %}
      "success":"true",
      "turn":"{{game.turn}}",
      "move":
      {
         "number":"{{move.number}}",
         "name":"{{move.name}}",
         "src_row":"{{move.src_row}}",
         "src_col":"{{move.src_col}}",
         "dest_row":"{{move.dest_row}}",
         "dest_col":"{{move.dest_col}}"
      }
   {% else %}
      "success":"false"
   {% endif %}
   
}
{% endautoescape %}
